{
  "name": "webpack-tools",
  "version": "3.0.0",
  "description": "HTML template",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --mode development --open ",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "prod": "cross-env NODE_ENV=production webpack --mode production && prettier --print-width=120 --parser html --write dist/*.html",
    "watch": "cross-env NODE_ENV=development webpack --mode development && cross-env NODE_ENV=development webpack --mode development --watch",
    "serv-prod": "http-server ./dist -g"
  },
  "dependencies": {
    "breakpoint-sass": "^2.7.1",
    "normalize.css": "^8.0.1",
    "npm": "^8.5.3",
    "petite-vue": "^0.4.1",
    "swiper": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.4",
    "compression-webpack-plugin": "^9.2.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.0",
    "cssnano": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "raw-loader": "^4.0.2",
    "sass-loader": "^12.6.0",
    "smart-grid": "^2.1.2",
    "style-loader": "^3.3.1",
    "svg-spritemap-webpack-plugin": "^4.4.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-spritesmith": "^1.1.0"
  }
}
